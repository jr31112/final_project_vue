                console.log(error)\r\n                })\r\n\r\n            this.credentials = {}\r\n        },\r\n      clear () {\r\n        this.credentials.username = ''\r\n        this.credentials.password = ''\r\n      },\r\n      signup (){\r\n          router.push({name:'signup'})\r\n      }\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\n    .LoginForm {\r\n        box-shadow: 5px 5px 5px 5px gray;\r\n        padding: 20px;\r\n        background-color: #ffffff;\r\n    }\r\n</style>"],"sourceRoot":"src/components"}]}