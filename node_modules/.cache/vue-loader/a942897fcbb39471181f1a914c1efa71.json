      console.log(response)\n                    const token = response.data.token\n                    this.$session.start()\n                    this.$session.set('jwt', token)\n                })\n                .catch(error => {\n                    console.log(error)\n                })\n\n            this.credentials = {}\n        }\n    }\n}\n</script>\n\n<style>\n\n</style>"]}]}